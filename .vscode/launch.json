{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test Download",
            "type": "debugpy",
            "request": "launch",
            "module": "srt.cli",
            "console": "integratedTerminal",
            "args": [
                "ds",
                "download",
                "tushare",
                "stock",
                "daily",
                "--symbols",
                "000001.SZ,000002.SZ,600083.SH",
                "--start-at",
                "2023-03-01:00:00:00",
                "--stop-at",
                "2023-03-31:00:00:00"
            ],
        },
        {
            "type": "debugpy",
            "request": "launch",
            "name": "Reset DB",
            "module": "srt.cli",
            "console": "integratedTerminal",
            "args": [
                "download",
                "reset-db-or-tables",
                "--reset-db"
            ],
        },
        {
            "name": "Run ds show",
            "type": "debugpy",
            "request": "launch",
            "module": "srt.cli",
            "console": "integratedTerminal",
            "args": [
                "ds",
                "show",
                "tushare",
                "stock_price_ohlcv_daily",
                "601088.SH",
                "--start-at",
                "1980-01-01:00:00:00",
            ],
        },
        {
            "name": "Run Pyramid Strategy",
            "type": "debugpy",
            "request": "launch",
            "module": "srt.research.pyramid.main",
        },
        {
            "name": "Python: Module CmdVar",
            "type": "debugpy",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "${command:extension.commandvariable.file.relativeDirDots}.${fileBasenameNoExtension}",
        }
    ]
}
